{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": ["put"],
      "route": "{partitionKey}/{collection}/{id}"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "cosmosDB",
      "direction": "in",
      "name": "document",
      "databaseName": "ChsPride",
      "collectionName": "{collection}",
      "connectionStringSetting": "CosmosDbConnectionString",
      "id": "{id}",
      "partitionKey": "{partitionKey}"
    },
    {
      "type": "cosmosDB",
      "direction": "out",
      "name": "outputDocument",
      "databaseName": "ChsPride",
      "collectionName": "{collection}",
      "createIfNotExists": false,
      "connectionStringSetting": "CosmosDbConnectionString",
      "partitionKey": "{partitionKey}"
    }
  ]
}
