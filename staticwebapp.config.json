{
  "routes": [
    {
      "route": "/.auth/login/aad",
      "statusCode": 404
    },
    {
      "route": "/protected",
      "allowedRoles": ["authenticated"]
    },
    {
      "route": "/api/directors/*",
      "allowedRoles": ["authenticated"]
    }
  ],
  "navigationFallback": {
    "rewrite": "index.html",
    "exclude": ["*.{png,jpg,gif,svg}", "*.css"]
  }
}
