{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": ["post"],
      "route": "{collection}"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "cosmosDB",
      "direction": "out",
      "name": "outputDocument",
      "databaseName": "ChsPride",
      "collectionName": "{collection}",
      "createIfNotExists": false,
      "connectionStringSetting": "CosmosDbConnectionString",
      "partitionKey": "chspride"
    }
  ]
}
